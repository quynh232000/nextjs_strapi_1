{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    // "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/types/*": ["src/types/*"],
      "@/providers/*": ["src/providers/*"],
      "@/styles/*": ["src/styles/*"],
      "@/utils/*": ["src/utils/*"],
      "@/images/*": ["src/images/*"],
      "@/atoms/*": ["src/components/atoms/*"],
      "@/templates/*": ["src/components/templates/*"],
      "@/dp__atoms/*": ["src/design-pattern/atoms/*"],
      "@/dp__molecules/*": ["src/design-pattern/molecules/*"],
      "@/dp__organisms/*": ["src/design-pattern/organisms/*"],
      "@/dp__templates/*": ["src/design-pattern/templates/*"],
      "@/services/*": ["src/services/*"]
    },
    "strict": false,
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
